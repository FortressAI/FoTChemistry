:hasCoherence rdf:type owl:DatatypeProperty ;
              rdfs:domain :VQbitState ;
              rdfs:range xsd:double ;
              rdfs:label "has coherence" ;
              rdfs:comment "Quantum coherence value of a vQbit state (0.0 to 1.0)" .

###  http://fieldoftruth.org/ontology/chemistry#hasConfidence
:hasConfidence rdf:type owl:DatatypeProperty ;
               rdfs:domain :Claim ;
               rdfs:range xsd:double ;
               rdfs:label "has confidence" ;
               rdfs:comment "Confidence level of a claim (0.0 to 1.0)" .

###  http://fieldoftruth.org/ontology/chemistry#hasFidelity
:hasFidelity rdf:type owl:DatatypeProperty ;
             rdfs:domain :VQbitState ;
             rdfs:range xsd:double ;
             rdfs:label "has fidelity" ;
             rdfs:comment "Quantum fidelity of a vQbit state measurement" .

