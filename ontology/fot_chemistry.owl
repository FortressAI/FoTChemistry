###  http://fieldoftruth.org/ontology/chemistry#hasComponent
:hasComponent rdf:type owl:ObjectProperty ;
              rdfs:domain :ChemicalEntity ;
              rdfs:range :ChemicalEntity ;
              rdfs:label "has component" ;
              rdfs:comment "Relates a chemical entity to its component parts" .

###  http://fieldoftruth.org/ontology/chemistry#hasCondition
:hasCondition rdf:type owl:ObjectProperty ;
              rdfs:domain :Reaction ;
              rdfs:range :Condition ;
              rdfs:label "has condition" ;
              rdfs:comment "Relates a reaction to its experimental conditions" .

###  http://fieldoftruth.org/ontology/chemistry#hasEvidence
:hasEvidence rdf:type owl:ObjectProperty ;
             rdfs:domain :Claim ;
             rdfs:range :Evidence ;
             rdfs:label "has evidence" ;
             rdfs:comment "Relates a claim to supporting evidence" .

###  http://fieldoftruth.org/ontology/chemistry#hasMeasurement
:hasMeasurement rdf:type owl:ObjectProperty ;
                rdfs:domain :ChemicalEntity ;
                rdfs:range :Measurement ;
                rdfs:label "has measurement" ;
                rdfs:comment "Relates a chemical entity to experimental measurements" .

###  http://fieldoftruth.org/ontology/chemistry#hasProduct
:hasProduct rdf:type owl:ObjectProperty ;
            rdfs:domain :Reaction ;
            rdfs:range :Molecule ;
            rdfs:label "has product" ;
            rdfs:comment "Relates a reaction to its products" .

###  http://fieldoftruth.org/ontology/chemistry#hasReactant
:hasReactant rdf:type owl:ObjectProperty ;
             rdfs:domain :Reaction ;
             rdfs:range :Molecule ;
             rdfs:label "has reactant" ;
             rdfs:comment "Relates a reaction to its reactants" .

###  http://fieldoftruth.org/ontology/chemistry#hasQuantumState
:hasQuantumState rdf:type owl:ObjectProperty ;
                 rdfs:domain :ChemicalEntity ;
                 rdfs:range :VQbitState ;
                 rdfs:label "has quantum state" ;
                 rdfs:comment "Relates a chemical entity to its vQbit quantum representation" .

###  http://fieldoftruth.org/ontology/chemistry#isEntangledWith
:isEntangledWith rdf:type owl:ObjectProperty ;
                 rdf:type owl:SymmetricProperty ;
                 rdfs:domain :VQbitState ;
                 rdfs:range :VQbitState ;
                 rdfs:label "is entangled with" ;
                 rdfs:comment "Symmetric relation between quantum-entangled vQbit states" .

###  http://fieldoftruth.org/ontology/chemistry#validatedBy
:validatedBy rdf:type owl:ObjectProperty ;
             rdfs:domain :Claim ;
             rdfs:range :Agent ;
             rdfs:label "validated by" ;
             rdfs:comment "Indicates which agent validated a claim" .

