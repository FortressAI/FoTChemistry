@prefix : <http://fieldoftruth.org/ontology/chemistry#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix prov: <http://www.w3.org/ns/prov#> .

# Example molecules for FoTChemistry knowledge graph

# Water molecule
:water rdf:type :Molecule ;
       rdfs:label "Water" ;
       :hasSMILES "O" ;
       :hasInChI "InChI=1S/H2O/h1H2" ;
       :hasComponent :hydrogen_atom , :oxygen_atom ;
       prov:generatedAtTime "2025-09-28T12:00:00Z"^^xsd:dateTime .

# Ethanol molecule  
:ethanol rdf:type :Molecule ;
         rdfs:label "Ethanol" ;
         :hasSMILES "CCO" ;
         :hasInChI "InChI=1S/C2H6O/c1-2-3/h3H,2H2,1H3" ;
         prov:generatedAtTime "2025-09-28T12:00:00Z"^^xsd:dateTime .

# Aspirin molecule
:aspirin rdf:type :Molecule ;
         rdfs:label "Aspirin" ;
         :hasSMILES "CC(=O)OC1=CC=CC=C1C(=O)O" ;
         :hasInChI "InChI=1S/C9H8O4/c1-6(10)13-8-5-3-2-4-7(8)9(11)12/h2-5H,1H3,(H,11,12)" ;
         prov:generatedAtTime "2025-09-28T12:00:00Z"^^xsd:dateTime .

# Example reaction: Esterification
:esterification rdf:type :Reaction ;
                rdfs:label "Esterification of acetic acid and ethanol" ;
                :hasReactant :acetic_acid , :ethanol ;
                :hasProduct :ethyl_acetate , :water ;
                :hasYield 85.0 ;
                prov:generatedAtTime "2025-09-28T12:00:00Z"^^xsd:dateTime .

# Reactants
:acetic_acid rdf:type :Molecule ;
             rdfs:label "Acetic acid" ;
             :hasSMILES "CC(=O)O" ;
             :hasInChI "InChI=1S/C2H4O2/c1-2(3)4/h1H3,(H,3,4)" .

# Products  
:ethyl_acetate rdf:type :Molecule ;
               rdfs:label "Ethyl acetate" ;
               :hasSMILES "CC(=O)OCC" ;
               :hasInChI "InChI=1S/C4H8O2/c1-3-6-4(2)5/h3H2,1-2H3" .

# Example experimental conditions
:room_temperature rdf:type :Condition ;
                  rdfs:label "Room temperature" ;
                  :hasTemperature 298.15 .

:acidic_conditions rdf:type :Condition ;
                   rdfs:label "Acidic conditions" ;
                   rdfs:comment "Sulfuric acid catalyst" .

# Link reaction to conditions
:esterification :hasCondition :room_temperature , :acidic_conditions .

# Example measurement
:pka_measurement rdf:type :Measurement ;
                rdfs:label "pKa measurement of aspirin" ;
                prov:used :aspirin ;
                prov:generatedAtTime "2025-09-28T12:00:00Z"^^xsd:dateTime .

# Example vQbit quantum state
:aspirin_vqbit rdf:type :VQbitState ;
              rdfs:label "Aspirin quantum state" ;
              :hasCoherence 0.73 ;
              :hasFidelity 0.91 ;
              prov:wasGeneratedBy :quantum_calculation .

:aspirin :hasQuantumState :aspirin_vqbit .

# Example quantum calculation activity
:quantum_calculation rdf:type prov:Activity ;
                    rdfs:label "Quantum state calculation" ;
                    prov:startedAtTime "2025-09-28T11:55:00Z"^^xsd:dateTime ;
                    prov:endedAtTime "2025-09-28T12:00:00Z"^^xsd:dateTime ;
                    prov:wasAssociatedWith :measurement_agent .

# Example agents
:measurement_agent rdf:type :Agent ;
                  rdfs:label "Measurement Agent" ;
                  rdfs:comment "Agent responsible for quantum measurements" .

:validation_agent rdf:type :Agent ;
                 rdfs:label "Validation Agent" ;
                 rdfs:comment "Agent responsible for data validation" .

# Example claim about aspirin pKa
:aspirin_pka_claim rdf:type :Claim ;
                  rdfs:label "Aspirin pKa = 3.5 Â± 0.2" ;
                  :hasConfidence 0.87 ;
                  :hasVirtueWeight 0.82 ;
                  :hasEvidence :literature_reference_1 ;
                  :validatedBy :validation_agent ;
                  prov:generatedAtTime "2025-09-28T12:00:00Z"^^xsd:dateTime .

# Example evidence
:literature_reference_1 rdf:type :Evidence ;
                       rdfs:label "Literature reference for aspirin pKa" ;
                       rdfs:comment "Experimental determination in aqueous solution" ;
                       prov:generatedAtTime "2025-09-28T12:00:00Z"^^xsd:dateTime .
